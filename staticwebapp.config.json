{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*",
            "/_framework/*",
            "/css/*",
            "/.auth/*"
        ]
    },
    "routes": [
        {
            "route": "/.auth/login/done",
            "rewrite": "/.auth/login/done/index.html"
        },
        {
            "route": "/admin/*",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/*",
            "headers": {
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
            }
        }
    ],
    "platform": {
        "apiRuntime": "node:20"
    }
}